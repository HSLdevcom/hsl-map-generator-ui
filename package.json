{
  "name": "hsl-map-generator-ui",
  "productName": "MapGenerator",
  "version": "0.0.1",
  "description": "Map Generator application based on React, React Router, Webpack, React Hot Loader for rapid application development",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-register --recursive --require ./test/setup.js test/**/*.spec.js",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test mocha --compilers js:babel-register --require ./test/setup.js --require co-mocha ./test/e2e.js",
    "lint": "eslint app test *.js",
    "clean": "rimraf dist",
    "bundle": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js",
    "build": "npm run clean && npm run bundle",
    "start-dev": "cross-env NODE_ENV=development node -r babel-register server-dev.js",
    "start-prod": "cross-env NODE_ENV=production node -r babel-register server.js",
    "start": "npm run start-dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HSLdevcom/hsl-map-generator-ui.git"
  },
  "author": {
    "name": "Hannes Junnila",
    "email": "hannes.junnila@gmail.com",
    "url": "https://github.com/hannesj"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HSLdevcom/hsl-map-generator-ui/issues"
  },
  "keywords": [
    "react",
    "react-router",
    "flux",
    "webpack",
    "react-hot"
  ],
  "homepage": "https://github.com/HSLdevcom/hsl-map-generator-ui#readme",
