{
    "name": "hsl-map-generator-ui",
    "productName": "MapGenerator",
    "version": "0.0.1",
    "description": "Map Generator application based on React, Webpack, React Hot Loader for rapid application development",
    "main": "index.js",
    "scripts": {
        "test": "cross-env NODE_ENV=test mocha --compilers js:babel-register --recursive --require ./test/setup.js test/**/*.spec.js",
        "test-watch": "npm test -- --watch",
        "test-e2e": "cross-env NODE_ENV=test mocha --compilers js:babel-register --require ./test/setup.js --require co-mocha ./test/e2e.js",
        "lint": "eslint app",
        "codestyle": "prettier \"./**/*.{css,js}\" --write",
        "clean": "rimraf dist",
        "bundle": "cross-env NODE_ENV=production webpack --config webpack.config.production.js",
        "bundle:dev": "cross-env NODE_ENV=development webpack --config webpack.config.development.js",
        "build": "yarn clean && yarn lint && yarn bundle",
        "build:dev": "yarn clean && yarn lint && yarn bundle:dev",
        "start": "cross-env NODE_ENV=development node server.js",
        "serve": "serve --port=3000 dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/HSLdevcom/hsl-map-generator-ui.git"
    },
    "author": {
        "name": "Hannes Junnila",
        "email": "hannes.junnila@gmail.com",
        "url": "https://github.com/hannesj"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/HSLdevcom/hsl-map-generator-ui/issues"
    },
    "keywords": [
        "react",
        "flux",
        "webpack",
        "react-hot"
    ],
    "homepage": "https://github.com/HSLdevcom/hsl-map-generator-ui#readme",
    "devDependencies": {
        "babel-eslint": "^6.0.4",
        "babel-loader": "^6.2.7",
        "babel-plugin-webpack-loaders": "^0.5.0",
        "babel-polyfill": "^6.9.0",
        "babel-preset-es2015": "^6.9.0",
        "babel-preset-react": "^6.5.0",
        "babel-preset-react-hmre": "^1.1.1",
        "babel-preset-stage-0": "^6.5.0",
        "babel-register": "^6.9.0",
        "brfs": "^2.0.2",
        "chai": "^3.5.0",
        "chromedriver": "^2.21.2",
